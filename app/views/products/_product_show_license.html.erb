<div class="box header">
	<h5>
		License
	</h5>
</div>
<div class="box content" >
	<% licenses = @product.licenses %>
	<% if licenses && !licenses.empty? %>
		<% licenses.each do |license| %>
			<% if license.url && !license.url.empty? %>
				<a href="<%= license.url %>" target="license"><%= license.name %></a>
			<% else %>
				<%= license.name %>
			<% end %>
		<% end %>
	<% else %>
		UNKNOWN
	<% end %>
</div>
