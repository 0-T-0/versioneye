<div class="boxHeader">
	Links
</div>
<div class="boxContainer" >
		
	<% if @product.version_link && !@product.version_link.empty? && @product.prod_type.eql?("Maven2") %>	
		<a href="<%= @product.version_link %>" target="new">Link to project</a><br/>
	<% end %>
	<% if !@product.link.nil? && !@product.link.empty? && @product.prod_type.eql?("RubyGem") %>
		<a href="<%= @product.link %>" target="new">RubyGem Page</a><br/>
	<% end %>
	<% if !@product.link.nil? && !@product.link.empty? && @product.prod_type.eql?("PIP") %>
		<a href="<%= @product.link %>" target="new">PIP Page</a><br/>
	<% end %>
	<% if !@product.link.nil? && !@product.link.empty? && @product.prod_type.eql?("GitHub") %>
		<a href="<%= @product.link %>" target="new">GitHub</a><br/>
	<% end %>
	<% if !@product.link.nil? && !@product.link.empty? && @product.prod_type.eql?("R") %>
		<a href="<%= @product.link %>" target="new">R Package</a><br/>
	<% end %>
	<% if !@product.get_links.empty? %>
		<% @product.get_links.each do |link| %>
			<a href="<%= link.get_link %>" target="new"><%= link.name %></a><br/>
		<% end %>
	<% end %>
	<% if !@product.get_version_links.empty? %>
		<% @product.get_version_links.each do |link| %>
			<a href="<%= link.get_link %>" target="new"><%= link.name %></a><br/>
		<% end %>
	<% end %>
	
</div>