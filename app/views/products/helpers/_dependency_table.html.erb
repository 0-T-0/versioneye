  
  <table class="dependency_table" style="width: 570px;">
    <thead>
      <tr>
        <th class="name_col"></th>
        <th class="version_col">Dependency Version</th>
        <th class="version_col">Current Version</th>
      </tr>
    </thead>
    <% dependencies.each do |dep| %>
      <% tr_class = "flash warn" if dep.outdated? %>
      <tr class="<%= tr_class %>">
        <td>
          <span itemprop="itemListElement" itemtype="http://schema.org/SoftwareApplication" itemscope="" >
            <a itemprop="url" class="#a dep_link" href="/package/<%= dep.dep_prod_key_for_url %>/version/<%= dep.version_for_url %>" >
              <div class="icon_link icon16"></div>
              <span itemprop="name" ><%= dep.name %></span>
            </a>
          </span>
        </td>
        <td>
          <% if !dep.version_for_label.nil? && !dep.version_for_label.empty? %>
            <span itemprop="softwareVersion"><%= dep.version_for_label %></span>
          <% end %> 
        </td>
        <td>
          <% if !dep.product.nil? %> 
            <%= dep.product.newest_version.version %>
          <% else %> 
            UNKNOWN
          <% end %> 
        </td>
      </tr>
    <% end %>
  </table>
