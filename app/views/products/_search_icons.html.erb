<% if product.icon.nil? %>									
	<% if product.prod_type.eql? "RubyGem" %>
		<%= image_tag "prod_type/logo_ruby.png" %>
	<% elsif product.prod_type.eql? "Maven2" %>
		<%= image_tag "prod_type/logo_java.png" %>
	<% elsif product.prod_type.eql? "PIP" %>
		<%= image_tag "prod_type/logo_python.png" %>
	<% elsif product.prod_type.eql? "npm" %>
		<%= image_tag "prod_type/logo_npm.png" %>
	<% elsif !product.language.nil? && product.language.eql? "Clojure" %>
		<%= image_tag "prod_type/logo_clojure.png" %>
	<% elsif !product.language.nil? && product.language.eql? "PHP" %>
		<%= image_tag "prod_type/logo_php.png" %>
	<% elsif !product.language.nil? && product.language.eql? "JavaScript" %>
		<%= image_tag "prod_type/logo_javascript.png" %>
	<% else %>
		<%= image_tag "eye_off.png" %>
	<% end %>									
<% else %>
	<img src="<%= product.icon %>" width="57" height="57"></img>
<% end %>