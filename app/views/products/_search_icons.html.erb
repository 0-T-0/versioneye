<% if product.icon.nil? %>									
	<% if product.prod_type.eql? "RubyGem" %>
		<%= image_tag "prod_type/logo_ruby.png" %>
	<% elsif product.prod_type.eql? "Maven2" %>
		<%= image_tag "prod_type/logo_java.png" %>
	<% elsif product.prod_type.eql? "PIP" %>
		<%= image_tag "prod_type/logo_python.png" %>
	<% else %>
		<%= image_tag "eye_off.png" %>
	<% end %>									
<% else %>
	<img src="<%= product.icon %>" width="57" height="57"></img>
<% end %>