<h3><%= pluralize(@main_dependencies.count, "Require Dependencies") %></h3>
<% @main_dependencies.each do |dep| %>
	<span itemprop="itemListElement" itemtype="http://schema.org/SoftwareApplication" itemscope="">
		<a itemprop="url" class="#a dep_link" href="/package/<%= dep.dep_prod_key_for_url %>/version/<%= dep.version_for_url %>" >
			<span itemprop="name" ><%= dep.name %></span> : <span itemprop="softwareVersion"><%= dep.version_for_label %></span>
		</a>
	</span><br/>
<% end %>

